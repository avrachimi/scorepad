{
    "cli": {
        "version": ">= 12.4.1",
        "appVersionSource": "remote"
    },
    "build": {
        "development": {
            "developmentClient": true,
            "distribution": "internal"
        },
        "preview": {
            "distribution": "internal",
            "env": {
                "EXPO_PUBLIC_API_ENDPOINT": "https://scorepad.general.avrachimi.com"
            }
        },
        "production": {
            "autoIncrement": true,
            "env": {
                "EXPO_PUBLIC_API_ENDPOINT": "https://scorepad.general.avrachimi.com"
            }
        }
    },
    "submit": {
        "production": {}
    }
}
